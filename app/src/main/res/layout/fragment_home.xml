<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include
        android:id="@+id/commonHead"
        layout="@layout/common_head_layout" />

    <Button
        android:id="@+id/btn_to_register_page"
        style="@style/ButtonStyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/space_50"
        android:layout_marginTop="@dimen/space_60"
        android:layout_marginRight="@dimen/space_50"
        android:text="点我进入注册页面" />

    <Button
        android:id="@+id/btn_network"
        style="@style/ButtonStyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/space_50"
        android:layout_marginTop="@dimen/space_60"
        android:layout_marginRight="@dimen/space_50"
        android:text="点我进网络请求页面"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_other"
        style="@style/ButtonStyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/space_50"
        android:layout_marginTop="@dimen/space_60"
        android:layout_marginRight="@dimen/space_50"
        android:text="点我进入测试页面"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_status"
        style="@style/ButtonStyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/space_50"
        android:layout_marginTop="@dimen/space_60"
        android:layout_marginRight="@dimen/space_50"
        android:text="点我进入加载数据状态的页面"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/btn_to_setting_page"
        style="@style/ButtonStyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/space_50"
        android:layout_marginTop="@dimen/space_60"
        android:layout_marginRight="@dimen/space_50"
        android:text="点我进入设置页面" />

</LinearLayout>